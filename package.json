{
  "name": "keep-watch",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "start:all": "node --experimental-modules scripts/start.js",
    "lint": "next lint",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "db:setup": "node --experimental-modules scripts/setup-db.js",
    "db:start": "docker-compose up -d postgres",
    "db:stop": "docker-compose down postgres",
    "db:reset": "prisma migrate reset --force",
    "demo": "npx ts-node scripts/demo.ts",
    "docker:dev": "docker-compose up --build",
    "docker:dev:rebuild": "docker-compose up --build --force-recreate",
    "docker:down": "docker-compose down",
    "docker:clean": "docker-compose down -v"
  },
  "dependencies": {
    "@prisma/client": "^6.4.1",
    "dotenv": "^16.4.7",
    "next": "15.1.7",
    "pg": "^8.11.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/pg": "^8.10.9",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.1.7",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
